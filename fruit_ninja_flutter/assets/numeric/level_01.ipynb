    "L = lower_bounds.to_numpy() * SCALAR1 * YEAR * 365  \n",
    "U = upper_bounds.to_numpy() * SCALAR2 * YEAR * 365\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# 回溯算法搜索所有可行解\n",
    "def backtrack_solution(index, current_solution, all_solutions):\n",
    "    if index == len(food_names):\n",
    "        # 检查当前解是否满足约束条件\n",
    "        if is_feasible_solution(current_solution):\n",
    "            all_solutions.append(current_solution.copy())\n",
    "        return\n",
    "\n",
    "    # 尝试不同的数量对于当前食物\n",
    "    for amount in range(max_count + 1):  # 假设 max_count 是食物的最大数量\n",
